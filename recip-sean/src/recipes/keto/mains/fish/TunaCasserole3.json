    "id": "keto-main_tuna_casserole_iii_001",
    "name": "Tuna Casserole III",
    "ingredients": [
      "1 stalk celery, diced",
      "2 tablespoons butter",
      "4 ounce can mushrooms, drained",
      "12 ounces frozen cauliflower",
      "2 5-ounce cans tuna, drained",
      "8 ounces cheddar cheese, shredded",
      "1/2 cup heavy cream",
      "1/4 cup mayonnaise",
      "1/2 teaspoon salt, or to taste",
      "1/4 teaspoon pepper, or to taste",
      "1/4 teaspoon onion powder",
      "1 teaspoon dry minced onion"
    ],
    "utensils": [
      "Small skillet",
      "1 1/2-quart casserole dish",
      "Microwave",
      "Oven"
    ],
    "cookingTime": 40,
    "prepTime": 15,
    "instructions": [
      {
        "time": 5,
        "en": "Sauté the diced celery with the butter in a small skillet until very tender and slightly browned.",
        "primaryIngredient": "celery"
      },
      {
        "time": 3,
        "en": "Add the drained mushrooms and cook until they're a little browned on the edges.",
        "primaryIngredient": "mushrooms"
      },
      {
        "time": 10,
        "en": "Put the frozen cauliflower in a 1 1/2-quart casserole dish. Cover and microwave on HIGH for 8-10 minutes until very tender.",
        "primaryIngredient": "cauliflower"
      },
      {
        "time": 5,
        "en": "Mix all of the remaining ingredients into the cauliflower. Add the celery and mushrooms.",
        "primaryIngredient": "tuna"
      },
      {
        "time": 30,
        "en": "Bake at 350°F for 20-30 minutes or until bubbly and browned on the top.",
        "primaryIngredient": "casserole"
      }
    ],
